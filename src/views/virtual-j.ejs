  <script src="/socket.io/socket.io.js"></script>
  <script>
    
    ///////////////////////////////////////VARIABLES////////////////////////////////////////////////////
    //prod
    //const socket = io("https://virtual-j.herokuapp.com")
    //testing
    const socket = io("http://localhost:4000/")
    
    //used for Toggle_Action() counter
    i=0

    //get all action buttons
    const actions = document.querySelectorAll('[id^=action_')
    
    //Store at the client that J is Asleep
    clientJAsleep = false

    //gifs
    const hungerGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-HUNGRY-CHIBI-02.gif?raw=true"
    const sleepyGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-SLEEPY-CHIBI-02.gif?raw=true"
    const asleepGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-ASLEEP-CHIBI-06.gif?raw=true"
    const eatingGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-EATING-CHIBI-02.gif?raw=true"
    const   idleGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-IDlE-CHIBI-01.gif?raw=true"
    const exerciseGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-EXERCISE-CHIBI-02.gif?raw=true"
    const lazyGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-LAZY-CHIBI-01.gif?raw=true"
    const boredGif = "https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-BORED-CHIBI-01.gif?raw=true";
    const playGif = "	https://github.com/MeganFuhr/BingaGifs/blob/main/JGifs/J-PLAY-CHIBI-01.gif?raw=true";
    //Gif array
    let gifCollection = []

    //Gif Index counter
    let gifIndex = 0;  // This will keep track of the current array index to use

    //HTML Element for Gifs
    let img = document.getElementById('gifs'); // Get your reference just once, not on each function call

    //update year in footer
    var year = new Date() 
    document.getElementById("footer").innerHTML = "&copy; Megan Fuhr " + year.getFullYear()
    //////////////////////////ALL GIFS///////////////////////

    //change gifs
    autoChange();

    //receive what should be in the gif array from server
    socket.on('update-all-clients-gifs', function(evt) {
      gifCollection = []
      evt.gifs.forEach(element => {
        console.log(`From the server, this element is added : ${element}`)
        gifCollection.push(element)
      })     
    })

    /////////////////////////HUNGER///////////////////////

    //if server sends false, J is not hungry.  if server sends true, j is hungry.
    socket.on('state-hungry', function(evt) {
      //j is not hungry
      if (evt.state === 'false') {
        document.getElementById('state_hungry').classList.remove('state_visible')
        document.getElementById('state_hungry').classList.add('state_invisible')
        console.log("J Isn't hungry received from server.....")
        serverMessage(evt) 
      }
      //j is hungry
      else {
        document.getElementById('state_hungry').classList.add('state_visible')
        document.getElementById('state_hungry').classList.remove('state_invisible')
      }
    })
    /////////////////////////Lazy///////////////////////
    socket.on('state-lazy', function(evt) {
      //j is not lazy
      if (evt.state === 'false') {
        document.getElementById('state_lazy').classList.remove('state_visible')
        document.getElementById('state_lazy').classList.add('state_invisible')
        serverMessage(evt) 
      }
      //j is lazy
      else {
        document.getElementById('state_lazy').classList.add('state_visible')
        document.getElementById('state_lazy').classList.remove('state_invisible')
      }
    })
    socket.on('state-bored', function(evt) {
      //j is not lazy
      if (evt.state === 'false') {
        document.getElementById('state_bored').classList.remove('state_visible')
        document.getElementById('state_bored').classList.add('state_invisible')
        serverMessage(evt) 
      }
      //j is lazy
      else {
        document.getElementById('state_bored').classList.add('state_visible')
        document.getElementById('state_bored').classList.remove('state_invisible')
      }
    })

    /////////////////////////SLEEP///////////////////////
      socket.on('state-sleepy', function(evt) {
      //j is not sleepy
      if (evt.state === 'false') {
        document.getElementById('state_sleepy').classList.remove('state_visible')
        document.getElementById('state_sleepy').classList.add('state_invisible')
        serverMessage(evt) 
        clientJAsleep = false
        return
      }
      //j is sleepy
      if (evt.state === 'true'){
        console.log(('client: j is sleepy'))
        document.getElementById('state_sleepy').classList.add('state_visible')
        document.getElementById('state_sleepy').classList.remove('state_invisible')
        serverMessage(evt) 
        return
      }
      //j already asleep
      if (evt.state === 'alreadyAsleep') {
        serverMessage(evt) 
        return
      }
      //j isn't sleepy
      if (evt.state === 'jIsntSleepy') {
        serverMessage(evt) 
        return
      }
    })


    //receive event from server that J is not hungry and display a message
    socket.on('jNotSleepy', function (evt) {
        serverMessage(evt)    
    })

    //receive event from server that J is already asleep and display a message
    socket.on('jIsAsleep', function(evt) {
      serverMessage(evt)  
    })

    //receive event from server that J is tired and the sleep icon should be visible.
    socket.on('j-is-sleepy', function(evt) {
      document.getElementById('state_tired').classList.add('state_visible')
    })

    //receive event from server that a client has fed J, so we need to tell this client that 
    //j is no longer hungry
    socket.on('update-all-clients-fed', function(evt) {
      //console.log that a client fed J - message from server
      document.getElementById('state_hungry').classList.remove('state_visible')
      document.getElementById('state_hungry').classList.add('state_invisible')

    })
    socket.on('update-all-clients-exercise', function(evt) {
      //update classes to show J is tired
      console.log(evt.message + ' - server told all a client J is no longery lazy')
      document.getElementById('state_lazy').classList.remove('state_visible')
      document.getElementById('state_lazy').classList.add('state_invisible')  
    })
    socket.on('update-all-clients-play', function(evt) {
      //update classes to show J is tired
      console.log(evt.message + ' - server told all a client J is no longer bored')
      document.getElementById('state_bored').classList.remove('state_visible')
      document.getElementById('state_bored').classList.add('state_invisible')  
    })

    //receive event from the server that a client put J to sleep
    socket.on('update-all-clients-sleep', function(evt) {
      //update classes to show J is tired
      console.log(evt.message + ' - server told all a client put J to sleep')
      document.getElementById('state_sleepy').classList.remove('state_visible')
      document.getElementById('state_sleepy').classList.add('state_invisible')
      clientJAsleep = true     
    })

    socket.on('update-client-j-already-asleep', function(evt) {
      //update classes to show J is tired
      console.log(evt.message)
      document.getElementById('state_tired').classList.remove('state_visible')
      document.getElementById('state_tired').classList.add('state_invisible')
      serverMessage(evt)
      
    })

    ///////////////////////////////////////FUNCTIONS////////////////////////////////////////////////////

    //receive messages from server.  Fades out.
    function serverMessage(evt) {
      document.getElementById('server_message').classList.remove('server_message_invisible')
      document.getElementById('server_message').innerHTML = evt.message
      document.getElementById('server_message').classList.add('server_message_visible')
      let serverMessageTimer = setTimeout(() => 
        {
          document.getElementById('server_message').classList.remove('server_message_visible')
          document.getElementById('server_message').classList.add('server_message_invisible')
        }, 1000     
      )
    }

    //swap gif immediately once called
    function swapGif(gif) {
        document.getElementById('gifs').src=gif
    }

    //auto rotate through array of gifs 
    //https://stackoverflow.com/questions/50788951/loop-through-image-array-with-pure-javascript
    function autoChange() {
      //what is in the array when called
      console.log(`Inside autochange: ${gifCollection}`)
      // You only need to ensure that the index isn't out of bounds
      if(gifIndex < gifCollection.length){
        img.src = gifCollection[gifIndex];      // If not, use the index
        gifIndex++;                        // Then, increment it
      } else {
        gifIndex = 0;  // If so, reset the index
      }
      // Now that the right image is showing, wait 2 seconds and call the function all over again
      setTimeout(autoChange, 2000);  
    }

    function Select_Action() {
      //Client Action - Feed J
      if(document.getElementById('action_food').classList.contains('action_visible')) {
        //only feed J if he is hungry
        if (document.getElementById('state_hungry').classList.contains('state_visible')){
          if(clientJAsleep === true){

            return
          }else {
            //remove hungerGif
            gifCollection = [eatingGif]

            //swap gif for eating gif
            swapGif(eatingGif)

            //tell the server
            socket.emit('action-fed', false)  
          }
        }
        else {
          serverMessage({message: "J isn't hungry right now."})
        }     
      }
      //Client Action - Exercise J
      if(document.getElementById('action_exercise').classList.contains('action_visible')) {
        if (document.getElementById('state_lazy').classList.contains('state_visible')){

          //remove lazyGif
          gifCollection = [exerciseGif]
        
          //swap gif for asleep gif
          swapGif(exerciseGif)

          //tell the server
          socket.emit('action-lazy', false)
        }
        else {
          serverMessage({message: "J isn't lazy right now."})
        }   
      }
      //Client Action - Play Games
      if(document.getElementById('action_play').classList.contains('action_visible')) {
        if (document.getElementById('state_bored').classList.contains('state_visible')){

          //remove lazyGif
          gifCollection = [playGif]
        
          //swap gif for asleep gif
          swapGif(playGif)

          //tell the server
          socket.emit('action-play', false)
        }
        else {
          serverMessage({message: "J isn't bored right now."})
        }   
      }
      //Client Action - Put J to sleep
      if(document.getElementById('action_sleep').classList.contains('action_visible')) {
        if (document.getElementById('state_sleepy').classList.contains('state_visible')){

          //remove sleepyGif
          gifCollection = [asleepGif]
        
          //swap gif for asleep gif
          swapGif(asleepGif)

          //tell the server
          socket.emit('action-sleep', false)
        }
        else {
          serverMessage({message: "J isn't sleepy right now."})
        }   
      }
    //not hungry - keeping this just to test out placement o fserverMessage
    //if(document.getElementById('action_food').classList.contains('action_invisible')) {
    // serverMessage({message: "J isn't hungry right now."})
    //}
   }
    

    function Toggle_Action() {
      switch(i) {
        case 0:
          i=1            
          //eat
          actions[0].classList.add('action_visible')

          actions[1].classList.remove('action_visible')
          actions[2].classList.remove('action_visible')
          actions[3].classList.remove('action_visible')
          i++
          break;
        case 2:
          //exercise
          i=2
          actions[1].classList.add('action_visible')

          actions[0].classList.remove('action_visible')
          actions[2].classList.remove('action_visible')
          actions[3].classList.remove('action_visible')
          i++
          break;
        case 3:
          //Play
          i=3
          actions[2].classList.add('action_visible')

          actions[0].classList.remove('action_visible')
          actions[1].classList.remove('action_visible')
          actions[3].classList.remove('action_visible')
          i++
          break;
        case 4:
          //sleep
          i=4
          actions[3].classList.add('action_visible')

          actions[0].classList.remove('action_visible')
          actions[1].classList.remove('action_visible')
          actions[2].classList.remove('action_visible')
          i=0
          break;
      }
    }
    //https://stackoverflow.com/questions/10643426/how-to-add-a-tooltip-to-an-svg-graphic
    //for hovering over buttons - show tool tip
    function showTooltip(evt, text) {
        let tooltip = document.getElementById("tooltip");
        tooltip.innerHTML = text;
        tooltip.style.display = "block";
        tooltip.style.left = evt.pageX + 7 + 'px';
        tooltip.style.top = evt.pageY - 30 + 'px';
    }

    function hideTooltip() {
      var tooltip = document.getElementById("tooltip");
      tooltip.style.display = "none";
    }
    </script>
